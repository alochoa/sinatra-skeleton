<div id="formbox" class="col-md-6">
	<div class="center">
		<form method="Post" action="/signup">
		<fieldset>
		<legend>Sign Up</legend>
			<p class="form-group">
				<label for="Name-field">First Name</label>
				<input type="text" name="first_name" id="Name-field">
			</p>
			<p class="form-group">
				<label for="Last-field">Last Name</label>
				<input type="text" name="last_Name" id="Last-field">
			</p>
			<p class="form-group">
				<label for="Email-field">Email</label>
				<input type="text" name="email" id="Email-field">
			</p>
			<p class="form-group">
				<label for="Password-field">Password</label>
				<input type="password" name="password" id="Password-field">
			</p>
			<p class="form-group">
				<label for="Retype-field">Retype Password</label>
				<input type="password" name="retype" id="retype-field">
			</p>
			<p class="form-group">
				<label for="Howdidyou-field">How did you hear about us?</label>
				<select name="howdid">
					<option value="Friend">Friend</option>
					<option value="Search">Search</option>
					<option value="Fairy">Fairy</option>
					<option value="Ghost">Ghost</option>
				</select>
				<!-- <input list="Howdidyou-field" name="Howdidyou">
				<datalist id="Howdidyou-field">
					<option value="Friend">
					<option value="Search">
					<option value="Fairy">
					<option value="Ghost">
				</datalist> -->
			</p>
			<p class="form-group">
				<label for="age-field">I am at least 18yrs old</label>
				<input type="checkbox" name="agetickbox" id="age-field" required>
			</p>
			<button type="submit" class="btn btn-default">Sign me up!</button>
		</fieldset>
		</form>
		</div>
</div>